{
  "name": "@zod-jwt/jwt-kms-hs-provider",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Adam Smith",
  "description": "Sign, Verify, and Decode JWTs with Zod and AWS KMS",
  "homepage": "https://github.com/zod-jwt/zod-jwt",
  "keywords": ["zod", "jwt", "kms", "aws"],
  "repository": {
    "type": "git",
    "url": "https://github.com/zod-jwt/zod-jwt"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "build": "pnpm run clean && pnpm run compile",
    "test": "pnpm vitest run --reporter verbose"
  },
  "peerDependencies": {
    "@zod-jwt/core": "workspace:*",
    "@aws-sdk/client-kms": ">=3",
    "@aws-sdk/types": ">=3"
  }
}
